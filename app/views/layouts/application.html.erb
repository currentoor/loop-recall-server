  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

  <link href="http://loop-recall-assets.s3-us-west-1.amazonaws.com/css/style.css" rel="stylesheet" type="text/css">
  <link rel="stylesheet" href="http://loop-recall-assets.s3-us-west-1.amazonaws.com/css/grid12.css" media="all">
  <link rel="stylesheet" href="http://loop-recall-assets.s3-us-west-1.amazonaws.com/css/solarized_dark.min.css" media="all">

  <link rel="apple-touch-icon" sizes="57x57" href="/apple-touch-icon-57x57.png">
  <link rel="apple-touch-icon" sizes="60x60" href="/apple-touch-icon-60x60.png">
